{
    "name": "phlexible/focal-point-bundle",
    "description": "phlexible focal point bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\FocalPointBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
